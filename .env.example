# Interactive Brokers Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# ============================================================================
# CONNECTION SETTINGS - Trader Workstation (TWS) or IB Gateway
# ============================================================================
# Trader Workstation (TWS):
#   Paper Trading:  Port 7497 (default)
#   Live Trading:   Port 7496
#
# IB Gateway (lighter alternative):
#   Paper Trading:  Port 4002
#   Live Trading:   Port 4001
# ============================================================================

IB_HOST=127.0.0.1
IB_PORT=7497
IB_CLIENT_ID=1

# IB Account Credentials
IB_USERNAME=your_username_here
IB_PASSWORD=your_password_here

# Account Information
IB_ACCOUNT_ID=your_account_id_here

# Trading Mode
# Options: paper, live
TRADING_MODE=paper

# Logging Level
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
